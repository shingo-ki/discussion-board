<ul class="list-unstyled">
  <h2><%= 'ユーザ一覧' %></h2>
  <% @users.each do |user| %>
    <li class="media">
      <div class="media-body">
        <div>
          <h5><p><%= link_to user.id , user_path(user) %> : <%= link_to user.name, user_path(user) %>さん</p></h5>
        </div>
      </div>
    </li>
  <% end %>
</ul>
<%= paginate @users %>
